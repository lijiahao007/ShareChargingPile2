                    app:layout_constraintTop_toBottomOf="@+id/materialDivider2"
                    app:layout_constraintVertical_bias="0.58"
                    android:layout_marginEnd="2dp"
                    tools:text="广东省汕头市金平区汕头大学" />

                <TextView
